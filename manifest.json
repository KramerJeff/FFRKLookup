{
  "manifest_version": 2,

  "name": "FFRK Lookup",
  "short_name": "FFRKLookup",
  "description": "This extension is used as a quick reference tool for the Enlir/Community FFRK spreadsheet",
  "version": "1.1.1",
  "browser_action": {
   "default_icon": "images/tyro.png",
   "default_popup": "index.html"
  },
  "icons": {
    "16": "images/tyro-16.png",
    "48": "images/tyro-48.png",
    "128": "images/tyro-128.png",
    "256": "images/tyro.png"
  },
  "content_security_policy": "script-src 'self' https://code.jquery.com/; object-src 'self'",
  "permissions": [
   "activeTab"
   ]
}
